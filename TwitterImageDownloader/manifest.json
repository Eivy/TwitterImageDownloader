{
  "description": "Help you to download images from twitter timeline",
  "manifest_version": 2,
  "name": "TwitterImageDownloader",
  "version": "0.9",
  "homepage_url": "https://github.com/Eivy/TwitterImageDownloader",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "TwitterImageDownloader",
    "default_icon": {
      "16": "index.svg",
      "32": "index.svg"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "storage",
    "webRequest",
    "*://*.twitter.com/*"
  ],
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png"
  }
}
